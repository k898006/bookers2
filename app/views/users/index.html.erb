<div>
  <%= render 'show', user: @user %>
  <%= render 'books/new', book: @book %>
</div>

<h1>Users</h1>
<% @users.each do |user| %>
<table>
    <tr>
        <th>image</th>
        <th>name</th>
        <th></th>
    </tr>
    <tr>
        <td><%= image_tag user.get_profile_image(63, 63) %></td>
        <td><%= user.name %></td>
        <td><%= link_to "Show", user_path(user.id) %></td>
    </tr>
</table>
<% end %>
